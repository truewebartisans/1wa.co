    "url": "https://github.com/truewebartisans/1wa.co/issues"
  },
  "homepage": "https://github.com/truewebartisans/1wa.co#readme",
  "devDependencies": {
    "animate.css": "^3.7.2",
    "cssnano": "^4.1.10",
    "eslint": "^6.7.2",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
