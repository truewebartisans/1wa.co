{
  "name": "1wa.co",
  "version": "1.0.1",
  "description": "True web artisans site.",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clear": "rm -rf ../static",
    "start": "parcel ./src/pug/index.pug --open",
    "build": "parcel build ./src/pug/*.pug -d ../static --public-url ./",
    "copy:common": "cp ./src/common/* ../static",
    "build:prod": "npm run clear && npm run build && npm run copy:common"
  },
